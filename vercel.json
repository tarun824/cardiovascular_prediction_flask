{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
      "excludeFiles": [
        "**/__pycache__/**",
        "**/*.pyc",
        "**/tests/**"
    ]
}